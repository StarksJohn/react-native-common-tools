{
  "name": "react-native-common-tools",
  "version": "1.0.47",
  "description": "Contains some modules commonly used in react native",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/StarksJohn/react-native-tools.git"
  },
  "author": "stark",
  "bugs": {
    "url": "https://github.com/StarksJohn/react-native-tools/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "homepage": "https://github.com/StarksJohn/react-native-tools#readme",
  "module": "dist/react-native-common-tools2.esm.js",
  "size-limit": [
    {
      "path": "dist/react-native-common-tools2.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-native-common-tools2.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.5",
    "@react-native-community/eslint-config": "^3.0.0",
    "@react-navigation/core": "^6.1.0",
    "@react-navigation/native": "^6.0.6",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "clean-deep": "^3.4.0",
    "dva-core": "^2.0.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "lodash": "^4.17.21",
    "prop-types": "^15.7.2",
    "qs": "^6.10.1",
    "react-native": "0.63.4",
    "react-native-easy-app": "^1.7.27",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-largelist": "^3.1.0-rc.2",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-spring-scrollview": "^4.0.0-dev-0",
    "starkfrontendtools": "^0.0.8",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.3",
    "@types/lodash": "^4.14.172",
    "@types/react-native": "^0.66.6",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  }
}
